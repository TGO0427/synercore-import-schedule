{
  "name": "synercore-import-schedule",
  "version": "1.0.0",
  "description": "Import Supply Chain Management system for Synercore",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "tsx server/index.js",
    "migrate": "node server/db/migrate-consolidated.js",
    "migrate:status": "node server/db/migrate-consolidated.js --status",
    "migrate:reset": "node server/db/migrate-consolidated.js --reset",
    "migrate:legacy": "node server/db/migrate.js",
    "migrate:rejection": "node server/db/add-rejection-migration.js",
    "migrate:notifications": "node server/db/add-notifications-tables.js",
    "setup:db": "npm run migrate",
    "backfill-week-dates": "node server/db/backfill-week-dates.js",
    "create-admin": "node server/scripts/createAdminUser.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:server": "NODE_OPTIONS='--experimental-vm-modules' jest server",
    "test:utils": "NODE_OPTIONS='--experimental-vm-modules' jest server/utils",
    "test:middleware": "NODE_OPTIONS='--experimental-vm-modules' jest server/middleware",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:auth": "cypress run --spec 'cypress/e2e/auth.cy.js'",
    "test:e2e:shipments": "cypress run --spec 'cypress/e2e/shipments.cy.js'",
    "test:e2e:accessibility": "cypress run --spec 'cypress/e2e/accessibility.cy.js'",
    "test:e2e:performance": "cypress run --spec 'cypress/e2e/performance.cy.js'",
    "test:all": "npm run test && npm run test:e2e:headless"
  },
  "keywords": [
    "shipment",
    "schedule",
    "import",
    "logistics"
  ],
  "author": "Synercore",
  "license": "ISC",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.26.0",
    "@sentry/react": "^10.27.0",
    "@sentry/tracing": "^7.120.4",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "cron-parser": "^5.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.3.2",
    "helmet": "^8.1.0",
    "imap": "^0.8.17",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "mailparser": "^3.7.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "react-chartjs-2": "^5.3.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "web-vitals": "^5.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwt-decode": "^2.2.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.2.0",
    "cypress": "^15.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^7.1.4",
    "terser": "^5.31.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
